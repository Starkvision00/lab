<body>
<div class="item">item1</div>
<div class="item">item2</div>
<div class="item">item3</div>
<div class="item">item4</div>
<div class="item">item5</div>

<script>
$(function(){
 let i=$(".item");
 console.log("First:",i.first().text());
 console.log("Third:",i.eq(2).text());
 console.log("Even index first:",i.filter((k)=>k%2===0).first().text());
});
</script>
